{
  "name": "activiti-vs-code-bpmn-io",
  "displayName": "Activiti BPMN Editor",
  "description": "Edit BPMN 2.0 files. Based on bpmn.io tools.",
  "license": "MIT",
  "version": "0.14.1",
  "publisher": "adamDilger",
  "repository": "https://github.com/adamDilger/vs-code-bpmn-io/",
  "engines": {
    "vscode": "^1.63.1"
  },
  "icon": "resources/logo_marketplace.png",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:bpmn",
    "onCommand:extension.bpmn-io.edit",
    "onWebviewPanel:bpmn-io.edit"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "bpmn",
        "aliases": [
          "BPMN",
          "bpmn"
        ],
        "extensions": [
          ".bpmn2",
          ".bpmn",
          ".bpmn20.xml"
        ]
      }
    ],
    "commands": [
      {
        "command": "extension.bpmn-io.edit",
        "title": "Open BPMN Modeler",
        "icon": {
          "light": "./resources/icon_light.svg",
          "dark": "./resources/icon_dark.svg"
        }
      }
    ],
    "keybindings": [
      {
        "command": "extension.bpmn-io.edit",
        "key": "shift+ctrl+v",
        "mac": "shift+cmd+v",
        "when": "editorTextFocus && resourceLangId == bpmn"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "when": "resourceLangId == bpmn",
          "command": "extension.bpmn-io.edit"
        }
      ],
      "explorer/context": [
        {
          "when": "resourceLangId == bpmn",
          "command": "extension.bpmn-io.edit",
          "group": "navigation"
        }
      ],
      "editor/context": [
        {
          "when": "resourceLangId == bpmn",
          "command": "extension.bpmn-io.edit",
          "group": "navigation"
        }
      ],
      "editor/title": [
        {
          "when": "resourceLangId == bpmn",
          "command": "extension.bpmn-io.edit",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "all": "run-s test",
    "vscode:prepublish": "npm run compile",
    "compile": "run-s compile:clean compile:ts webpack",
    "compile:ts": "tsc -p ./",
    "compile:clean": "shx rm -rf out",
    "webpack": "webpack --config src/webview/webpack.config.js",
    "watch": "tsc -watch -p ./",
    "pretest": "run-s compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/chai": "^4.3.0",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "^17.0.7",
    "@types/shelljs": "^0.8.10",
    "@types/sinon": "^10.0.6",
    "@types/sinon-chai": "^3.2.8",
    "@types/vscode": "1.63.1",
    "chai": "^4.3.4",
    "glob": "^7.2.0",
    "mocha": "^9.1.3",
    "npm-run-all": "^4.1.5",
    "shelljs": "^0.8.4",
    "shx": "^0.3.3",
    "sinon": "^12.0.1",
    "sinon-chai": "^3.7.0",
    "tslint": "^5.20.1",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-no-unused-expression-chai": "^0.1.4",
    "typescript": "^4.5.4",
    "vscode-test": "^1.6.1"
  },
  "dependencies": {
    "activiti-bpmn-moddle": "github:adamDilger/camunda-bpmn-moddle#activiti",
    "bpmn-js": "^8.8.3",
    "bpmn-js-properties-panel": "github:adamDilger/bpmn-js-properties-panel#activiti.0.x",
    "css-loader": "^6.6.0",
    "style-loader": "^3.3.1",
    "vscode-codicons": "^0.0.17",
    "webpack": "^5.68.0",
    "webpack-cli": "^4.9.2"
  }
}
